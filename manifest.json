{
    "name": "Jooble helper",
    "description" : "powered by MAS",
    "version": "4.0",
    "manifest_version": 2,
    "permissions": ["storage","activeTab"],
    "content_scripts": [{
          "matches": ["https://*.jooble.org/*", "https://jooblework.knack.com/*"],
          "run_at": "document_idle",
          "js": ["content.js"]
        }],
        "browser_action": {
          "default_icon": "img/jooble.png"
        }
        
  }